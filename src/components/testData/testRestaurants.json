          "RecipeId": 3,
          "UserId": 3
      },
      "Ingredients": [
          {
              "id": 51,
              "name": "1 fennel bulb (sometimes called anise), stalks discarded, bulb cut into 1/2-inch dice, and feathery leaves reserved for garnish",
              "createdAt": "2017-12-07T23:13:16.000Z",
              "updatedAt": "2017-12-07T23:13:16.000Z",
              "RecipeId": 3
          },
          {
              "id": 52,
              "name": "1 onion, diced",
              "createdAt": "2017-12-07T23:13:16.000Z",
              "updatedAt": "2017-12-07T23:13:16.000Z",
              "RecipeId": 3
          },
          {
              "id": 53,
              "name": "2 tablespoons unsalted butter",
              "createdAt": "2017-12-07T23:13:16.000Z",
              "updatedAt": "2017-12-07T23:13:16.000Z",
              "RecipeId": 3
          },
          {
              "id": 54,
              "name": "2 medium russet (baking) potatoes",
              "createdAt": "2017-12-07T23:13:16.000Z",
              "updatedAt": "2017-12-07T23:13:16.000Z",
              "RecipeId": 3
          },
          {
              "id": 55,
              "name": "2 cups chicken broth",
              "createdAt": "2017-12-07T23:13:16.000Z",
              "updatedAt": "2017-12-07T23:13:16.000Z",
              "RecipeId": 3
          },
          {
              "id": 56,
              "name": "1 1/2 cups milk",
              "createdAt": "2017-12-07T23:13:16.000Z",
              "updatedAt": "2017-12-07T23:13:16.000Z",
              "RecipeId": 3
          }
      ],
      "Directions": [
          {
              "id": 12,
              "description": "In a large heavy saucepan cook diced fennel and onion in butter over moderate heat, stirring, until softened, about 10 minutes. Peel and cube potatoes. Add potatoes and broth to fennel mixture and simmer, covered, until potatoes are very tender, about 20 minutes. In a blender or food processor purée mixture in batches until smooth and return to saucepan. Stir in milk and salt and pepper to taste and simmer soup, stirring occasionally, 10 minutes, or until heated through.",
              "dirOrder": 0,
              "createdAt": "2017-12-07T23:13:16.000Z",
              "updatedAt": "2017-12-07T23:13:16.000Z",
              "RecipeId": 3
          },
          {
              "id": 13,
              "description": "Garnish soup with reserved fennel leaves.",
              "dirOrder": 1,
              "createdAt": "2017-12-07T23:13:16.000Z",
              "updatedAt": "2017-12-07T23:13:16.000Z",
              "RecipeId": 3
          }
